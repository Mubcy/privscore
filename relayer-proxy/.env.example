# relayer-proxy/.env
PORT=3000
RELAYER_URL=https://relayer.zama.ai
# Optional if your relayer requires a key:
# RELAYER_API_KEY=
# Optional proxy auth key that frontend must include via X-Relayer-Proxy-Key header:
# RELAYER_PROXY_API_KEY=my-secret-proxy-key
# Optional: allowed origin for CORS (frontend)
# ALLOWED_ORIGIN=http://localhost:5173
